ometa CalcParser {
  digit    = ^digit:d                 -> d,
  number   = number:n digit:d         -> (+ (* n 10) d)
           | digit,
  addExpr  = addExpr:x '+' mulExpr:y  -> `(add ,x ,y)
           | addExpr:x '-' mulExpr:y  -> `(sub ,x ,y)
           | mulExpr,
  mulExpr  = mulExpr:x '*' primExpr:y -> `(mul ,x ,y)
           | mulExpr:x '/' primExpr:y -> `(div ,x ,y)
           | primExpr,
  primExpr = '(' expr:x ')'           -> x
           | number:n                 -> `(num ,n),
  expr     = addExpr
}